{
  "name": "@pixel/components",
  "version": "0.0.2",
  "description": "Pixel Styled Stencil WebComponents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pixel-components/pixel-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "run-s build:lib build:docs",
    "build:lib": "stencil build",
    "build:docs": "madoc build",
    "dev": "run-p dev:lib dev:docs",
    "dev:lib": "stencil build --dev --watch",
    "dev:docs": "madoc dev",
    "test": "stencil test --spec --coverage",
    "test.watch": "stencil test --spec --watchAll",
    "serve:docs": "madoc serve"
  },
  "devDependencies": {
    "@madoc/core": "0.0.13",
    "@pixel/fonts": "0.0.1",
    "@pixel/icons": "0.0.8",
    "@stencil/core": "1.12.2",
    "@stencil/sass": "1.3.2",
    "@types/jest": "24.9.1",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "npm-run-all": "4.1.5",
    "svelte": "3.23.2"
  },
  "license": "MIT",
  "dependencies": {}
}
